md-dialog(aria-label='Venue')
    md-toolbar
      .md-toolbar-tools
        h2(translate="VENUE.TITLE")
        span(flex='')
        md-button.md-icon-button(ng-click='vm.cancel()')
          md-icon(md-svg-src='img/icons/ic_close_24px.svg', aria-label='Close dialog')

    md-dialog-content
      .md-dialog-content
        venue-form(venue="vm.venue", on-cancel="vm.cancel()", on-save="vm.save()")

