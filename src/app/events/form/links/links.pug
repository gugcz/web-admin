md-list
  md-list-item(ng-repeat='link in vm.links')
    md-input-container(flex)
      label(translate='EVENTS.FORM.SOCIAL_LINK')
      //md-icon(md-svg-icon='mdi:google-plus-box')
      //md-icon(md-svg-icon='mdi:facebook-plus-box')
      md-icon(md-svg-icon='mdi:link-variant')

      input(ng-model='link.url', ng-change='vm.checkEmptyItem()')
    md-icon(ng-click='vm.removeLink($index)').md-secondary close
